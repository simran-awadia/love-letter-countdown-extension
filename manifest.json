{
  "name": "Countdown",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.boardgamearena.com/*/loveletter*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "tabs"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
